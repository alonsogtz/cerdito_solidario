<div class="page-header">
  <h3>User #<%= @user.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Email</dt>
      <dd><%= @user.email %></dd>

      <dt>Password</dt>
      <dd><%= @user.password %></dd>

      <dt>Given name</dt>
      <dd><%= @user.given_name %></dd>

      <dt>Paternal last name</dt>
      <dd><%= @user.paternal_last_name %></dd>

      <dt>Maternal last name</dt>
      <dd><%= @user.maternal_last_name %></dd>

      <dt>Curp</dt>
      <dd><%= @user.curp %></dd>

      <dt>Curp image</dt>
      <dd><%= @user.curp_image %></dd>

      <dt>Proof of adress image</dt>
      <dd><%= @user.proof_of_adress_image %></dd>

      <dt>Address</dt>
      <dd><%= @user.address %></dd>

      <dt>Birth day</dt>
      <dd><%= @user.birth_day %></dd>

      <dt>Phone</dt>
      <dd><%= @user.phone %></dd>

      <dt>Comments</dt>
      <dd><%= @user.comments %></dd>

      <dt>Validation</dt>
      <dd><%= @user.validation %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/users" class="btn btn-primary">
        Back
      </a>
    </div>
  </div>
</div>
