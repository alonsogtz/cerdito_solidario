<div class="page-header">
  <h3>Additional Info #<%= @additional_info.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Description</dt>
      <dd><%= @additional_info.description %></dd>

      <dt>Image</dt>
      <dd><img src="<%= @additional_info.image %>" class="img-responsive"></dd>

      <dt>User</dt>
      <dd><%= @additional_info.user_id %></dd>

      <dt>Title</dt>
      <dd><%= @additional_info.title %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/additional_infos" class="btn btn-primary">
        Back
      </a>
      <a href="/additional_infos/<%= @additional_info.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_additional_info/<%= @additional_info.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
